# MyMusicPlayer
My music player - Project week 2-3

****************************************************************************************************
*** Entering this script in sql server management will give the user to get our database aswell. ***
*** All the songs from the media folder is entered in our database and, also some playlists      ***
****************************************************************************************************


USE [master]
GO
/****** Object:  Database [MyMediaPlayer]    Script Date: 18-01-2019 09:49:26 ******/
CREATE DATABASE [MyMediaPlayer]
 CONTAINMENT = NONE
 ON  PRIMARY 
( NAME = N'MyMediaPlayer', FILENAME = N'C:\Program Files\Microsoft SQL Server\MSSQL14.MSSQLSERVER\MSSQL\DATA\MyMediaPlayer.mdf' , SIZE = 8192KB , MAXSIZE = UNLIMITED, FILEGROWTH = 65536KB )
 LOG ON 
( NAME = N'MyMediaPlayer_log', FILENAME = N'C:\Program Files\Microsoft SQL Server\MSSQL14.MSSQLSERVER\MSSQL\DATA\MyMediaPlayer_log.ldf' , SIZE = 8192KB , MAXSIZE = 2048GB , FILEGROWTH = 65536KB )
GO
ALTER DATABASE [MyMediaPlayer] SET COMPATIBILITY_LEVEL = 140
GO
IF (1 = FULLTEXTSERVICEPROPERTY('IsFullTextInstalled'))
begin
EXEC [MyMediaPlayer].[dbo].[sp_fulltext_database] @action = 'enable'
end
GO
ALTER DATABASE [MyMediaPlayer] SET ANSI_NULL_DEFAULT OFF 
GO
ALTER DATABASE [MyMediaPlayer] SET ANSI_NULLS OFF 
GO
ALTER DATABASE [MyMediaPlayer] SET ANSI_PADDING OFF 
GO
ALTER DATABASE [MyMediaPlayer] SET ANSI_WARNINGS OFF 
GO
ALTER DATABASE [MyMediaPlayer] SET ARITHABORT OFF 
GO
ALTER DATABASE [MyMediaPlayer] SET AUTO_CLOSE OFF 
GO
ALTER DATABASE [MyMediaPlayer] SET AUTO_SHRINK OFF 
GO
ALTER DATABASE [MyMediaPlayer] SET AUTO_UPDATE_STATISTICS ON 
GO
ALTER DATABASE [MyMediaPlayer] SET CURSOR_CLOSE_ON_COMMIT OFF 
GO
ALTER DATABASE [MyMediaPlayer] SET CURSOR_DEFAULT  GLOBAL 
GO
ALTER DATABASE [MyMediaPlayer] SET CONCAT_NULL_YIELDS_NULL OFF 
GO
ALTER DATABASE [MyMediaPlayer] SET NUMERIC_ROUNDABORT OFF 
GO
ALTER DATABASE [MyMediaPlayer] SET QUOTED_IDENTIFIER OFF 
GO
ALTER DATABASE [MyMediaPlayer] SET RECURSIVE_TRIGGERS OFF 
GO
ALTER DATABASE [MyMediaPlayer] SET  DISABLE_BROKER 
GO
ALTER DATABASE [MyMediaPlayer] SET AUTO_UPDATE_STATISTICS_ASYNC OFF 
GO
ALTER DATABASE [MyMediaPlayer] SET DATE_CORRELATION_OPTIMIZATION OFF 
GO
ALTER DATABASE [MyMediaPlayer] SET TRUSTWORTHY OFF 
GO
ALTER DATABASE [MyMediaPlayer] SET ALLOW_SNAPSHOT_ISOLATION OFF 
GO
ALTER DATABASE [MyMediaPlayer] SET PARAMETERIZATION SIMPLE 
GO
ALTER DATABASE [MyMediaPlayer] SET READ_COMMITTED_SNAPSHOT OFF 
GO
ALTER DATABASE [MyMediaPlayer] SET HONOR_BROKER_PRIORITY OFF 
GO
ALTER DATABASE [MyMediaPlayer] SET RECOVERY FULL 
GO
ALTER DATABASE [MyMediaPlayer] SET  MULTI_USER 
GO
ALTER DATABASE [MyMediaPlayer] SET PAGE_VERIFY CHECKSUM  
GO
ALTER DATABASE [MyMediaPlayer] SET DB_CHAINING OFF 
GO
ALTER DATABASE [MyMediaPlayer] SET FILESTREAM( NON_TRANSACTED_ACCESS = OFF ) 
GO
ALTER DATABASE [MyMediaPlayer] SET TARGET_RECOVERY_TIME = 60 SECONDS 
GO
ALTER DATABASE [MyMediaPlayer] SET DELAYED_DURABILITY = DISABLED 
GO
EXEC sys.sp_db_vardecimal_storage_format N'MyMediaPlayer', N'ON'
GO
ALTER DATABASE [MyMediaPlayer] SET QUERY_STORE = OFF
GO
USE [MyMediaPlayer]
GO
/****** Object:  User [workPC]    Script Date: 18-01-2019 09:49:27 ******/
CREATE USER [workPC] FOR LOGIN [workPC] WITH DEFAULT_SCHEMA=[dbo]
GO
/****** Object:  Table [dbo].[tblPlaylist]    Script Date: 18-01-2019 09:49:27 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE TABLE [dbo].[tblPlaylist](
	[fldPlaylist] [nchar](50) NOT NULL,
	[fldIndex] [int] NOT NULL,
 CONSTRAINT [PK_tblPlaylist] PRIMARY KEY NONCLUSTERED 
(
	[fldPlaylist] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON) ON [PRIMARY]
) ON [PRIMARY]
GO
/****** Object:  Table [dbo].[tblPlaylistSong]    Script Date: 18-01-2019 09:49:27 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE TABLE [dbo].[tblPlaylistSong](
	[fldPlaylistName] [nchar](50) NULL,
	[fldSong] [nchar](30) NULL,
	[fldOrder] [int] NOT NULL,
	[fldIndex] [int] IDENTITY(0,1) NOT NULL,
 CONSTRAINT [PK_tblPlaylistSong] PRIMARY KEY CLUSTERED 
(
	[fldIndex] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON) ON [PRIMARY]
) ON [PRIMARY]
GO
/****** Object:  Table [dbo].[tblSongs]    Script Date: 18-01-2019 09:49:27 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE TABLE [dbo].[tblSongs](
	[fldPath] [nchar](100) NOT NULL,
	[fldArtist] [nchar](30) NULL,
	[fldSong] [nchar](30) NULL,
 CONSTRAINT [PK_tblSongs] PRIMARY KEY CLUSTERED 
(
	[fldPath] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON) ON [PRIMARY]
) ON [PRIMARY]
GO
SET ANSI_PADDING ON
GO
/****** Object:  Index [IX_tblSongs]    Script Date: 18-01-2019 09:49:27 ******/
CREATE UNIQUE NONCLUSTERED INDEX [IX_tblSongs] ON [dbo].[tblSongs]
(
	[fldSong] ASC
)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, SORT_IN_TEMPDB = OFF, IGNORE_DUP_KEY = OFF, DROP_EXISTING = OFF, ONLINE = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON) ON [PRIMARY]
GO
ALTER TABLE [dbo].[tblPlaylist]  WITH CHECK ADD  CONSTRAINT [FK_tblPlaylist_tblPlaylist] FOREIGN KEY([fldPlaylist])
REFERENCES [dbo].[tblPlaylist] ([fldPlaylist])
GO
ALTER TABLE [dbo].[tblPlaylist] CHECK CONSTRAINT [FK_tblPlaylist_tblPlaylist]
GO
ALTER TABLE [dbo].[tblPlaylistSong]  WITH NOCHECK ADD  CONSTRAINT [FK_tblPlaylistSong_tblPlaylist] FOREIGN KEY([fldPlaylistName])
REFERENCES [dbo].[tblPlaylist] ([fldPlaylist])
GO
ALTER TABLE [dbo].[tblPlaylistSong] NOCHECK CONSTRAINT [FK_tblPlaylistSong_tblPlaylist]
GO
ALTER TABLE [dbo].[tblPlaylistSong]  WITH CHECK ADD  CONSTRAINT [FK_tblPlaylistSong_tblSongs] FOREIGN KEY([fldSong])
REFERENCES [dbo].[tblSongs] ([fldSong])
GO
ALTER TABLE [dbo].[tblPlaylistSong] CHECK CONSTRAINT [FK_tblPlaylistSong_tblSongs]
GO
USE [master]
GO
ALTER DATABASE [MyMediaPlayer] SET  READ_WRITE 
GO
