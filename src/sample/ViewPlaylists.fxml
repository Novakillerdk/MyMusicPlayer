   <children>
      <ListView fx:id="viewAllPlaylists" layoutX="100.0" layoutY="75.0" prefHeight="200.0" prefWidth="200.0" />
      <Button fx:id="selectPlaylist" layoutX="155.0" layoutY="300.0" mnemonicParsing="false" onAction="#handlePlaylist" text="Select playlist" />
   </children>
</AnchorPane>
