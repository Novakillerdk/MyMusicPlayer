<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SearchSongs">
   <children>
      <Label layoutX="51.0" layoutY="65.0" text="Search for songs or artists" />
      <TextArea fx:id="listContent" layoutX="20.0" layoutY="82.0" prefHeight="200.0" prefWidth="200.0" />
      <TextField fx:id="getContent" layoutX="236.0" layoutY="200.0" promptText="song/Artist" />
      <Button fx:id="searchForContet" layoutX="236.0" layoutY="170.0" mnemonicParsing="false" onAction="#handleContent" text="Search for song" />
      <Button fx:id="searchForContent" layoutX="236.0" layoutY="139.0" mnemonicParsing="false" onAction="#handleContent" text="Search for artist" />
   </children>
</AnchorPane>
